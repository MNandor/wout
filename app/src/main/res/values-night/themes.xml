<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Wout" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/cedarchest</item>
        <item name="colorPrimaryVariant">@color/firebrick</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/buff</item>
        <item name="colorSecondaryVariant">@color/maizecrayola</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <!-- Customize your theme here. -->
        <item name="android:textColor">@color/buff</item>
        <item name="android:editTextColor">@color/buff</item>
        <item name="android:textColorHint">@color/maizecrayola</item>
    </style>
</resources>